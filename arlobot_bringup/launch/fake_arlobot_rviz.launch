<launch>

    <!-- Start fake arlobot -->
    <include file="$(find arlobot_bringup)/launch/fake_arlobot_minimal.launch"/>

    <!-- Start teleop node -->
    <include file="$(find arlobot_teleop)/launch/teleop.launch"/>

    <!-- Start rviz -->
    <node name="modelvisualisation" pkg="rviz" type="rviz" output="screen" args="-d $(find arlobot_bringup)/sim.rviz" />

  
</launch>
